{
  "name": "kuro-api",
  "version": "0.9.1-beta",
  "description": "REST Api for Kuro",
  "contributors": [
    {
      "name": "Tobias Lahmann",
      "email": "tobiaslahmann@gmail.com",
      "url": "https://github.com/tlahmann"
    },
