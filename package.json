      "name": "Timo Netzer",
      "email": "exodiquas@gmail.com",
      "url": "https://github.com/exodiquas/"
    }
  ],
  "main": "app.js",
  "scripts": {
    "mongo:start": "docker run --name realworld-mongo -p 27017:27017 mongo & sleep 5",
    "start": "ts-node ./app.ts",
    "dev": "nodemon --config ./nodemon-config.json ./app.ts",
    "test": "newman run ./tests/api-tests.postman.json -e ./tests/env-api-tests.postman.json",
    "stop": "lsof -ti :3000 | xargs kill",
    "server": "nodemon --watch server dist/app.js --exec babel-node",
    "forever": "forever start dist/app.js -l dist/foreverLog.log -o dist/foreverOut.log -e dist/foreverErr.log",
    "mongo:stop": "docker stop realworld-mongo && docker rm realworld-mongo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gothinkster/productionready-node-api.git"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.20.1",
    "cors": "2.7.1",
    "dotenv": "^8.2.0",
    "ejs": "2.4.1",
    "errorhandler": "1.4.3",
    "express": "4.18.2",
    "express-jwt": "3.3.0",
    "express-session": "^1.17.0",
    "jsonwebtoken": "7.1.9",
    "method-override": "2.3.10",
    "methods": "1.1.2",
    "mongoose-unique-validator": "1.0.2",
    "morgan": "1.9.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "path": "^0.12.7",
    "request": "2.69.0",
    "slug": "0.9.1",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/ejs": "^3.0.1",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.3",
    "@types/express-jwt": "^0.0.42",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/method-override": "^0.0.31",
    "@types/methods": "^1.1.1",
    "@types/mongoose": "^5.7.7",
    "@types/mongoose-unique-validator": "^1.0.3",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.2",
    "@types/nodemon": "^1.19.0",
    "@types/passport": "^1.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.4",
    "@types/slug": "^0.9.1",
    "@types/underscore": "^1.9.4",
    "babel-node": "0.0.1-security",
    "mongoose": "^5.9.5",
    "newman": "^3.8.2",
    "node-env-file": "^0.1.8",
    "nodemon": "^1.11.0",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  }
}
